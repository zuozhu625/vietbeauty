# 服务器配置
PORT=5002
NODE_ENV=production

# 数据库配置
DB_PATH=./data/medical.db

# 外部服务配置
N8N_WEBHOOK_URL=http://localhost:5678/webhook/medical
EXTERNAL_API_TIMEOUT=30000

# 安全配置
JWT_SECRET=your-super-secret-jwt-key-here
API_RATE_LIMIT=100

# 日志配置
LOG_LEVEL=info
LOG_FILE=./logs/backend.log

# 缓存配置
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
